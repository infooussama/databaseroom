        <activity android:name=".DetailLigne"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailTeleStation"
            android:screenOrientation="portrait"/>
        <activity android:name=".CalculeItineraire" android:screenOrientation="portrait"/>
        <activity android:name=".ListStation" android:screenOrientation="portrait"/>
        <activity android:name=".DetailBusStation" android:screenOrientation="portrait"/>
        <activity android:name=".DetailTramStation" android:screenOrientation="portrait"/>
        <activity android:name=".DetailMetroStation" android:screenOrientation="portrait"/>
        <activity android:name=".SplashScreen" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>