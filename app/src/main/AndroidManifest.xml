<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.relationbdd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_2_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RelationBdd">
        <activity android:name=".Mylocation"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ShowResultInmaps"
